<?xml version='1.0' encoding='utf-8'?>
<widget id="me.dragosh.app" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>My app</name>
    <description>
        App description
    </description>
    <author email="oancea.dragosh@gmail.com" href="http://dragosh.me/">
      Oancea Dragos
    </author>
    <content src="index.html" />
    <access origin="*" />
